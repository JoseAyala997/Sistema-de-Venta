	<queryString>
		<![CDATA[SELECT (SELECT nombre_producto FROM productos s WHERE s.idservicios = d.idservicios)as producto, c.fecha,(SELECT nombre FROM proveedor pro WHERE pro.idproveedor = d.idproveedor)as proveedor,
               d.precio,d.cantidad,c.nro_factura,d.sub_total,c.total,concat(u.nombre, ' ', u.apellido)as usuario
            FROM detalle_compra d INNER JOIN compras c  ON d.idcompra = c.idcompra
                inner join persona u on u.idpersona=c.idusuarios
                 WHERE c.fecha BETWEEN '$P{fecha_inicio}' AND '$P{fecha_fin}' AND c.estado='FINALIZADO' AND d.idproveedor='$P{idproveedor}' order by c.idcompra desc]]>
	</queryString>
