	<queryString>
		<![CDATA[SELECT v.idventa,d.idservicios,v.fecha, (SELECT nombre_servicio FROM servicios s WHERE s.idservicios = d.idservicios)as nombreservicio,
 d.precio,d.cantidad,v.nro_factura,total, CONCAT(p.nombre, ' ', p.apellido)as paciente,concat(u.nombre, ' ', u.apellido)as usuario
 FROM detalle_venta d INNER JOIN venta v ON d.idventa = v.idventa
 inner join persona p on p.idpersona=v.idpaciente
 inner join persona u on u.idpersona=v.idusuarios
 WHERE v.fecha
 BETWEEN $P{fecha_inicio} AND $P{fecha_termino} order by v.idventa desc]]>
	</queryString>
